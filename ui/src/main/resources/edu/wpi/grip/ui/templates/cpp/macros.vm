#macro(setter $source)
set$source.value()(#cType($source.type()) *$source.value())#end

#macro( func $step )
$tMeth.name($step.name())(#foreach($inp in $step.getInputs()) #cType($inp.type()) *$inp.name(), #end#foreach($out in $step.getOutputs()) #cType($out.type()) *#cName($out.name())#if($velocityCount <$step.getOutputs().size()), #end#end)#end

#macro ( newInput $input)
#if ($input.type().contains("CoreEnum"))
	int $input.name()$c = $input.value();
#elseif ($input.type().contains("Enum"))
	int $input.name()$c = $input.value();
#elseif ($input.type().equals("MaskSize") || $input.type().equals("BlurType"))
    String $input.name()$c = "${input.value()}";
#elseif ( $input.type().contains("Type") || $input.type().equals("Interpolation") ||$input.type() == "FlipCode" )
	int $input.name()$c = $input.value();
#elseif ($input.type().equals("String"))
	$input.type() $input.name()$c = "$input.value()";
#elseif ($input.type().equals("List"))
#set($inputEndValue = $input.value().length() - 1)
	double $input.name()${c}[] = {$input.value().substring(1,$inputEndValue)};
#elseif ($input.value().equals("null"))
	#cType($input.type()) $input.name()$c;
#elseif ($input.value().contains("source") || $input.value().contains("output"))
	#cType($input.type()) $input.name()$c= *$input.value();
#else
	#cType($input.type()) $input.name()$c = $input.value();
#end#end